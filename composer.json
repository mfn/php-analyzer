{
    "name": "mfn/php-analyzer",
    "description": "Framework for performaing static PHP code analysis",
    "type": "library",
    "keywords": [
        "source",
        "code",
        "analysis"
    ],
    "homepage": "http://github.com/mfn/php-analyzer",
    "authors": [
        {
            "name": "Markus Fischer",
            "email": "markus@fischer.name",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/mfn/php-analyzer/issues"
    },
    "license": "MIT",
    "require": {
        "php": ">=5.4.0",
        "mfn/util-simpleorderedmap": "0.0.2",
        "nikic/php-parser": "~1.0",
        "psr/log": "~1.0",
        "symfony/console": "~2.5"
    },
    "autoload": {
        "psr-4": {
            "Mfn\\PHP\\Analyzer\\": "lib/"
        }
    },
    "bin": [
      "bin/php_analyzer.php"
    ],
    "require-dev": {
        "phpunit/phpunit": "7.*",
        "mfn/php-reflection-gen": "0.0.4"
    }
}
